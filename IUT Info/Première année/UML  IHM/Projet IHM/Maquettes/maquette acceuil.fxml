<AnchorPane fx:id="boutonConexion" onMouseClicked="#onConnexionClicked" prefHeight="400.0" prefWidth="400.0" styleClass="anchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
