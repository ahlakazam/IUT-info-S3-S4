         </children>
      </GridPane>
      <Button fx:id="bouttonConnexion" layoutX="175.0" layoutY="239.0" mnemonicParsing="false" onAction="#actionConnexion" text="Connexion " />
      <Label layoutX="101.0" layoutY="295.0" prefHeight="31.0" prefWidth="131.0" styleClass="connexionLabel" text="pas encore inscrit ? " />
      <Hyperlink fx:id="onclickSubscribe" layoutX="244.0" layoutY="299.0" onAction="#goToSubscribe" onMouseClicked="#onInscriptionClicked" text="cliquer ici" />
   </children>
</AnchorPane>
